{
  "routes": [
    {
      "route": "/*.ts",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.tsx",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.mjs",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*"]
  }
}

