{
  "routes": [
    {
      "route": "/",
      "mimeType": "text/html"
    },
    {
      "route": "/index.html",
      "mimeType": "text/html"
    },
    {
      "route": "/static/js/*",
      "mimeType": "application/javascript"
    },
    {
      "route": "/static/css/*",
      "mimeType": "text/css"
    },
    {
      "route": "/*.ts",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.tsx",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.mjs",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Content-Type": "application/octet-stream"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/assets/*"]
  }
}


